{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "static/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "static/$1"
    }
  ]
}
